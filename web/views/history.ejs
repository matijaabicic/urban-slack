  <body>

    <!-- *****************************************************************************************************************
  	 HEADER navbar -->

     <% include ./partial/header %>

  <!-- ***************************************************************************************************************** -->

	<!-- *****************************************************************************************************************
	 HEADERWRAP
	 ***************************************************************************************************************** -->
	<div id="headerwrap">
	    <div class="container">
			<div class="row">
				<div class="col-lg-8 col-lg-offset-2">
          <h1>Version History</h1>
          <h5>A quick guide on how to use Urban Slack bot.</h5>
				</div>
			</div><!-- /row -->
	    </div> <!-- /container -->
	</div><!-- /headerwrap -->

	<!-- *****************************************************************************************************************
	 Release History
	 ***************************************************************************************************************** -->
   <div class="row centered">
     <div class="col-md-8 col-md-offset-2">
       <div class="table-responsive">
         <% if (releases.length > 0) { %>
         <table class="table">
           <% releases.forEach(function(release) { %>
             <tr>
               <td><a href="<%=release.html_url %>" target="_blank"> Release <%= release.tag_name %></a></td>
               <td><p><%=release.name %></p></td>
               <td><p><%=release.created_at %></p></td>
             </tr>
           <% }); %>
         </table>
         <% } else { %>
           <h1>PROBLEM OCCURED WHILE GETTING DATA FROM GITHUB</h1>
           <p>Head over to <a href="https://github.com/matijaabicic/urban-slack/releases" target="_blank">Urban Github Repo</a> and see the list while we deal with the problem</p>
         <% } %>
       </div> <!-- /table-responsive -->
     </div>
   </div> <!-- /row -->

	<!-- *****************************************************************************************************************
	 FOOTER -->

   <% include ./partial/footer %>

<!-- ***************************************************************************************************************** -->


